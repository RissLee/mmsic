{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "homepage": ".",
  "scripts": {
    "start": "yarn start-react & yarn start-electron",
    "start-electron": "electron .",
    "start-react": "cross-env PORT=3000 umi dev",
